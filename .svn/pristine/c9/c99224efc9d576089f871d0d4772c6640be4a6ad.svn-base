<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<table cellspacing="0" cellpadding="0" id="cust_finaning_info">
	<tr>
		<td colspan=4 class="td-even-line-separator">&nbsp;</td>
	</tr>
	<tr class="x-panel-table-div-title">
		<td class="x-panel-table-div-title" colspan="4">
		<div class="toggle-icon-expanded" title="折叠"
			toggleClass="tr-project-s"></div>
		客户基本信息</td>
	</tr>
	 <tr class="tr-contractect-info tr-odd">
		<td class="td-content-title" >注册地址:</td>
		<td class="td-content" >
			<input name="cust_info.taxRegAdd" value="${requestScope['cust_info.taxRegAdd']}" class="td-content-input td-content-readonly" type="text" readonly="readonly"/>
		</td>
		<td class="td-content-title" >法定代表:</td>
		<td class="td-content" >
			<input name="cust_info.legalRepresentative" value="${requestScope['cust_info.legalRepresentative']}" class="td-content-input td-content-readonly" readonly="readonly" type="text"/>
		</td>
	</tr>
	<tr class="tr-contractect-info tr-even">
		<td class="td-content-title" >企业实际控制人:</td>
		<td class="td-content" >
			<input name="cust_info.legalRepresentative1" value="${requestScope['cust_info.legalRepresentative']}" class="td-content-input td-content-readonly" type="text" readonly="readonly"/>
		</td>
		<td class="td-content-title" >注册资本:</td>
		<td class="td-content" >
			<input name="cust_info.regCapital" value="${requestScope['cust_info.regCapital']}" class="td-content-input td-content-readonly"   type="text" readonly="readonly"/>
		</td>
	</tr>
	<tr class="tr-contractect-info tr-odd">
		<td class="td-content-title" >企业性质:</td>
		<td class="td-content" >
			<input name="cust_info.ownership" value="${requestScope['cust_info.ownership']}" class="td-content-input td-content-readonly" type="text" readonly="readonly"/>
		</td>
		<td class="td-content-title" >企业规模:</td>
		<td class="td-content" >
			<input name="cust_info.custScale" value="${requestScope['cust_info.custScale']}" class="td-content-input td-content-readonly" type="text" readonly="readonly"/>
		</td>
	</tr>
	<%---购车融资信息 --%>
	 <tr>
		<td colspan=4 class="td-even-line-separator">&nbsp;</td>
	</tr>
	<tr class="x-panel-table-div-title">
		<td class="x-panel-table-div-title" colspan="4">
		<div class="toggle-icon-expanded" title="折叠"
			toggleClass="tr-project-sur"></div>
		购车融资信息</td>
	</tr>
	<tr class="tr-project-sur tr-odd">
		<td class="td-content" >购买车型:</td>
		<td class="td-content" >
			<input name="tablename.column7" class="td-content-input td-content-readonly" type="text" readonly="readonly"/>
		</td>
		<td class="td-content" >购买数量:</td>
		<td class="td-content" >
			<input name="tablename.column8" class="td-content-input td-content-readonly" type="text" readonly="readonly"/>
		</td>
	</tr>
	<tr class="tr-project-sur tr-even">
		<td class="td-content" >总车价:</td>
		<td class="td-content" >
			<input name="contract_condition.equipAmt" value="${requestScope['contract_condition.equipAmt']}" class="td-content-input td-content-readonly" type="text" readonly="readonly"/>
		</td>
		<td class="td-content" >起租比例:</td>
		<td class="td-content" >
			<input name="contract_condition.firstPaymentRatio" value="${requestScope['contract_condition.firstPaymentRatio']}" class="td-content-input td-content-readonly" type="text" readonly="readonly"/>
		</td>
	</tr>
	<tr class="tr-project-sur tr-odd">
		<td class="td-content" >融资金额（万元）:</td>
		<td class="td-content" >
			<input name="contract_condition.leaseAmt" value="${requestScope['contract_condition.leaseAmt']}" class="td-content-input td-content-readonly" type="text" readonly="readonly"/>
		</td>
		<td class="td-content" >融资期限（月）:</td>
		<td class="td-content" >
			<input name="contract_condition.leaseTerm" value="${requestScope['contract_condition.leaseTerm']}" class="td-content-input td-content-readonly" type="text" readonly="readonly"/>
		</td>
	</tr>
	<tr class="tr-project-sur tr-even">
		<td class="td-content" >费用项目:</td>
		<td class="td-content" >
			<input name="tablename.column13" class="td-content-input td-content-readonly" type="text" readonly="readonly"/>
		</td>
		<td class="td-content" >车辆规格:</td>
		<td class="td-content" >
			<input name="tablename.column14" class="td-content-input td-content-readonly" type="text" readonly="readonly"/>
		</td>
	</tr>
	</table>
	