								<w:sz w:val="24"/>
							</w:rPr>
							<w:t>第二条</w:t>
						</w:r>
						<w:r w:rsidRPr="00D74433">
							<w:rPr>
								<w:rFonts w:ascii="宋体" w:hAnsi="宋体" w:cs="宋体"/>
								<w:kern w:val="0"/>
								<w:sz w:val="24"/>
							</w:rPr>
							<w:t xml:space="preserve">　法院专递的法律效力</w:t>
