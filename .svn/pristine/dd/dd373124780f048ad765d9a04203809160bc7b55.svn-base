<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!--变更说明 -->
<table class="fillTable" cellspacing="0" cellpadding="0" width="50%" id="changedetils">
	<tr class="tr-odd">
		<td class="td-content-title" style="width:10%">变更时间：</td>
		<td class="td-content" colspan=3>
	    <input id="proj_other_info.projchangedate" name="proj_other_info.projchangedate" class="Wdate td-content-input td-content-readonly"  
             type="text" value="${requestScope['proj_other_info.projchangedate'] }"  onClick="WdatePicker(this,{readOnly:true})" readOnly require="true" label="变更时间" /><font class="required-content">*</font>
		</td>
	</tr>
	<tr class="tr-even">
		<td class="td-content-title">变更说明：</td>
		<td class="td-content" colspan=3>
		<textarea rows="5" cols="150" style="width:98%;float:left;"  require="true" label="变更说明"  name="proj_other_info.projchangememo">${requestScope['proj_other_info.projchangememo'] }</textarea>	</td>
	</tr>
	<tr class="tr-odd">
		<td class="td-content-title">变更前内容：</td>
		<td class="td-content" colspan=3>
		<textarea rows="5" cols="150" require="true" label="变更前内容" 
			style="width:98%;float:left;" name="proj_other_info.projchangebeformemo">${requestScope['proj_other_info.projchangebeformemo'] }</textarea>
				</td>
	</tr>
		<tr class="tr-even">
		<td class="td-content-title">变更后内容：</td>
		<td class="td-content" colspan=3>
		<textarea rows="5" cols="150" require="true" label="变更后内容" 
			style="width:98%;float:left;" name="proj_other_info.projchangeaftermemo">${requestScope['proj_other_info.projchangeaftermemo'] }</textarea>	</td>
	</tr>
	
</table>