<w:rPr>
<w:rFonts w:ascii="华文中宋" w:fareast="华文中宋" w:h-ansi="华文中宋" w:hint="fareast"/>
<wx:font wx:val="华文中宋"/>
<w:b/>
<w:color w:val="000000"/>
<w:sz w:val="24"/>
</w:rPr>
<w:t>第二条 业务流程</w:t>
</w:r>
</w:p>
<w:p wsp:rsidR="0068545C" wsp:rsidRPr="005F1D8E" wsp:rsidRDefault="0068545C" wsp:rsidP="0068545C">
<w:pPr>
<w:spacing w:line="420" w:line-rule="exact"/>
<w:rPr>
<w:rFonts w:ascii="华文中宋" w:fareast="华文中宋" w:h-ansi="华文中宋"/>
