<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<table cellspacing="0" cellpadding="0" class="fillTable" >
          <tr>
             <td class="x-panel-table-div-title" colspan=4><div class="toggle-icon-expanded" title="折叠" toggleClass="tr-proj-cust-info"></div>${requestScope['cust_info.custclass']}客户信息</td>
         </tr>
          <tr class="tr-proj-cust-info tr-even">
             <td class="td-content-title">身份证号码</td>
             <td class="td-content"><input name="cust_info_person.idcardno"  class="td-content-input td-content-readonly" readOnly type="text" value="${requestScope['cust_info_person.idcardno']}"></td>
             <td class="td-content-title">民族：</td>
            <td class="td-content"><input name="cust_info_person.custethnic" class="td-content-input td-content-readonly" readOnly  type="text"  value="${requestScope['cust_info_person.custethnic']}" /></td>
         </tr>
          <tr class="tr-proj-cust-info tr-odd">
             <td class="td-content-title">现住址：</td>
             <td class="td-content" colspan=5><input style="width:60%" name="cust_info_person.homeadd" readOnly  class="td-content-input td-content-readonly"  type="text" value="${requestScope['cust_info_person.homeadd']}" ></td>
          </tr>
          <tr class="tr-proj-cust-info tr-even">   
             <td class="td-content-title">手机号码：</td>
             <td class="td-content"><input  name="cust_info_person.mobilenumber" class="td-content-input td-content-readonly" readOnly   type="text" value="${requestScope['cust_info_person.mobilenumber'] }" ></td>
             <td class="td-content-title">工作单位：</td>
             <td class="td-content"><input name="cust_info_person.unitname" class="td-content-input td-content-readonly" readOnly  type="text"  value="${requestScope['cust_info_person.unitname'] }"></td>
         </tr>
           <tr class="tr-proj-cust-info tr-odd">
             <td class="td-content-title">获取驾照日期：</td>
             <td class="td-content"><input name="cust_info_person.driverlicensedate" class="td-content-input td-content-readonly" readOnly  type="text"  value="${requestScope['cust_info_person.driverlicensedate'] }"></input></td>
             <td class="td-content-title">配偶姓名：</td>
             <td class="td-content"><input name="cust_info_person.spousename" class="td-content-input td-content-readonly" readOnly  type="text"  value="${requestScope['cust_info_person.spousename'] }"></input></td>
          </tr>
          
   </table>