<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!--变更说明 -->
<table class="fillTable" cellspacing="0" cellpadding="0" id="proj_review_info">
	<tr class="tr-odd">
		<td class="td-content-title" style="width:10%">复议日期：</td>
		<td class="td-content" colspan=3>
		<input name="colunm1" class="Wdate td-content-input td-content-readonly" dataType="Date"
             type="text" value="${requestScope['colunm1'] }"  onClick="WdatePicker(this,{readOnly:true})" readOnly require="true" label="复议日期" /><font class="required-content">*</font></td>
	</tr>
	<tr class="tr-even">
		<td class="td-content-title">复议说明：</td>
		<td class="td-content" colspan=3>
		<textarea rows="5" cols="150" style="width:98%;float:left;"  require="true" label="复议说明"  name="colunm2">${requestScope['colunm2'] }</textarea>	</td>
	</tr>
	<tr class="tr-even">
		<td class="td-content-title">复议前内容：</td>
		<td class="td-content" colspan=3>
		<textarea rows="5" cols="150" require="true" label="复议前内容" 
			style="width:98%;float:left;" name="colunm3">${requestScope['colunm3'] }</textarea>
				</td>
	</tr>
		<tr class="tr-odd">
		<td class="td-content-title">复议后内容：</td>
		<td class="td-content" colspan=3>
		<textarea rows="5" cols="150" require="true" label="复议后内容" 
			style="width:98%;float:left;" name="colunm4">${requestScope['colunm4'] }</textarea>	</td>
	</tr>
	
</table>