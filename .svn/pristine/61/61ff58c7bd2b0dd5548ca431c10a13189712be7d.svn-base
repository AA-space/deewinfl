<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<table cellspacing="0" cellpadding="0" id="id_sent_info">
	<tr>
       <td class="x-panel-table-div-title" colspan=4><div class="toggle-icon-expanded" title="折叠" toggleClass="tr-other-info"></div>快递信息</td>
     </tr>
     <tr class="tr-other-info tr-even">   
             <td class="td-content-title">快递单号：</td><td class="td-content"><input  name="dealerguaranteeinfo.singlenumber" class="td-content-input " type="text"  value="${requestScope['dealerguaranteeinfo.singlenumber'] }" require="true" label="快递单号"><font class="required-content">*</font></td>
             <td class="td-content-title">寄送日期：</td><td class="td-content">
              <input name="dealerguaranteeinfo.singletime" dataType="Date" class="Wdate td-content-input td-content-readonly"  
             type="text" value="${requestScope['dealerguaranteeinfo.singletime'] }"  onClick="WdatePicker(this,{readOnly:true})" readOnly require="true" label="寄送日期" /><font class="required-content">*</font>
     </tr>
     <!-- tr class="tr-other-info tr-even">   
             <td class="td-content-title">备注：</td>
             <td class="td-content" colspan="3">
             <textarea rows="5" style="float:left;width: 98%"  label="备注"' 
			name="dealer_document_info.sendmemo">${requestScope['dealer_document_info.sendmemo'] }</textarea>
       </td>
     </tr-->
</table>