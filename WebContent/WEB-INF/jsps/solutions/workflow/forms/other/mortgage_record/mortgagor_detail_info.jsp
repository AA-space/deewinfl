<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<table cellspacing="0" cellpadding="0">
	<tr>
       <td class="x-panel-table-div-title" colspan=4><div class="toggle-icon-expanded" title="折叠" toggleClass="tr-other-mortgage"></div>抵押人</td>
     </tr>
      <tr class="tr-other-mortgage tr-even">   
             <td class="td-content-title" style="width:17%">名称：</td><td class="td-content" style="width:33%"><input  name="mortgagename" class="td-content-input " type="text"  value="${requestScope['mortgagename'] }" require="true" label="名称"><font class="required-content">*</font></td>
             <td class="td-content-title" style="width:17%">联系人：</td><td class="td-content" style="width:33%"><input name="mortgageperson" class="td-content-input " type="text"   value="${requestScope['mortgageperson'] }" require="true" label="联系人"><font class="required-content">*</font></td>
     </tr>
      <tr class="tr-other-mortgage tr-odd">   
            <td class="td-content-title">地址：</td><td class="td-content"><input  name="mortgageaddress" class="td-content-input " type="text"  value="${requestScope['mortgageaddress'] }"  label="地址"><font class="required-content">*</font></td>
            <td class="td-content-title">电话：</td><td class="td-content"><input name="mortgagephone" class="td-content-input " type="text"    value="${requestScope['mortgagephone'] }"  label="电话"><font class="required-content">*</font></td>
     </tr>
</table>