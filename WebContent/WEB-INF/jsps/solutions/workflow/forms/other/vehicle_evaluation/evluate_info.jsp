<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
   <table cellspacing="0" cellpadding="0"  id="proj_base_info">
         <tr>
             <td class="x-panel-table-div-title" colspan=4><div class="toggle-icon-expanded" title="折叠" toggleClass="tr-evluate-info"></div>评估结果</td>
          </tr>
          <tr class="tr-evluate-info tr-even">
             <td class="td-content-title" style="width:15%">评估价格：</td>
             <td class="td-content" style="width:35%">
                <input  name="contract_equip_revoke.evaluatemoney" class="td-content-input " type="text" value="${requestScope['contract_equip_revoke.evaluatemoney'] }" require="true" label="评估价格"><font class="required-content">*</font>
              </td>
             <td class="td-content-title" style="width:15%">评估时间：</td>
             <td class="td-content" style="width:35%">
                 <input name="contract_equip_revoke.evaluatedate" class="Wdate td-content-input td-content-readonly"  
                  type="text" value="${requestScope['contract_equip_revoke.evaluatedate'] }"  onClick="WdatePicker(this,{readOnly:true})" readOnly require="true" label="评估时间" /><font class="required-content">*</font></td>
          </tr>
           <tr class="tr-evluate-info tr-odd">
             <td class="td-content-title">备注：</td>
             <td class="td-content" colspan="3">
               <textarea rows="5"  require="true" label="备注"' style="width:90%;float:left" name="contract_equip_revoke.evaluateInfo">${requestScope['contract_equip_revoke.evaluateInfo'] }</textarea>
               <font class="required-content">*</font></td>
          </tr>
    </table>
        
	    