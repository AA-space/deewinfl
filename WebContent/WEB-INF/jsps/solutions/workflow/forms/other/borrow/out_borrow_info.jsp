<%@ page language="java" contentType="text/html; charset=UTF-8"    pageEncoding="UTF-8"%>
       
         <table id="borrow_info" cellspacing="0" cellpadding="0">
         <tr>
             <td class="x-panel-table-div-title" colspan=4>
                   外部借阅信息登记
             </td>
          </tr>
          <tr class="tr-contractect-info1 tr-even">
             <td class="td-content-title">借阅人姓名：</td>
             <td class="td-content">
             	<input name="idname" id="idname" class="td-content-input td-content-readonly" readOnly type="text"  value='${empty idname ? "" : idname }' require="true" label="借阅人姓名">
             <font class="required-content">*</font>
             </td>
             <td class="td-content-title">归还时间：</td><td class="td-content">
             <input name="regdate" class="Wdate td-content-input td-content-readonly"  
               type="text" onClick="WdatePicker(this,{readOnly:true})" readOnly  value='${empty regdate ? "" : regdate }' require="true" label="登记日期" /><font class="required-content">*</font>
             </td>
         </tr>
          
           <tr class="tr-contractect-info1 tr-odd">
             <td class="td-content-title">电话：</td><td class="td-content"><input name="telphone" id="telphone"  value='${empty telphone ? "" : telphone }' class="td-content-input td-content-readonly" require="true" type="text" label="电话">
             <font class="required-content">*</font></td>
             <td class="td-content-title">地址：</td><td class="td-content"><input name="address" id="address" class="td-content-input td-content-readonly"  value='${empty address ? "" : address }' require="true" type="text" label="地址">
             <font class="required-content">*</font>
             </td>
          </tr>
          <tr class="tr-contractect-info1 tr-even">  
             <td class="td-content-title">公司名称：</td><td class="td-content">
             <input name="companyname" id="companyname" value='${empty companyname ? "" : companyname }'  class="td-content-input  td-content-readonly" type="text">
             </td>
           </tr>
         
        </table>
      
        