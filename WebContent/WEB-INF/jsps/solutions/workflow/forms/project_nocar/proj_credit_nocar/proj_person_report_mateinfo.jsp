<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<div class="mini-panel" title="配偶情况" showCollapseButton="true" style="width: 100%;">
	<div id="mateinfoForm">
	    <table class="fillTable" cellspacing="0" cellpadding="0">
	
	          <tr class="tr-project-info tr-even">
	             <td class="td-content-title">姓名：</td><td class="td-content">
	             	<input name="mateinfo.a1" id =""  class="mini-textbox" value="${requestScope['mateinfo.a1'] }">
	             </td>
	             <td class="td-content-title">出生年月：</td><td class="td-content">
	             		<input name="mateinfo.a2" class="mini-datepicker" value="${requestScope['mateinfo.a2'] }"/>
	             </td>
	         </tr>
	          <tr class="tr-project-info tr-odd">
	             <td class="td-content-title">证件类型：</td>
	             <td class="td-content">
	             	<input name="mateinfo.a3" id =""  class="mini-textbox" value="${requestScope['mateinfo.a3'] }">
	             </td>
	             <td class="td-content-title">证件号码：</td>
	             <td class="td-content">
		             <input name="mateinfo.a4" id =""  class="mini-textbox" value="${requestScope['mateinfo.a4'] }">
	             </td>
	          </tr>
	          <tr class="tr-project-info tr-even">  
	             <td class="td-content-title">工作单位：</td>
	             <td class="td-content">
		             <input name="mateinfo.a5" id ="" value="${requestScope['mateinfo.a5'] }" class="mini-textbox">
	             </td>
	             <td class="td-content-title">单位地址：</td>
	             <td class="td-content">
		             <input name="mateinfo.a6" id ="" value="${requestScope['mateinfo.a6'] }" class="mini-textbox">
	             </td>
	          </tr>
	          <tr class="tr-project-info tr-odd">  
	             <td class="td-content-title">单位电话：</td>
	             <td class="td-content">
		             <input name="mateinfo.a7" id ="" value="${requestScope['mateinfo.a7'] }" class="mini-textbox">
	             </td>
	             <td class="td-content-title">邮政编码：</td>
	             <td class="td-content">
		             <input id="" name="mateinfo.a8" class="mini-textbox" value="${requestScope['mateinfo.a8'] }"/>
	             </td>
	          </tr>
	          <tr class="tr-project-info tr-even">
	             <td class="td-content-title">其他联系方式：</td>
	             <td class="td-content" colspan=3>
	             	<input name="mateinfo.a9" id="" class="mini-textbox" value="${requestScope['mateinfo.a9'] }"/>
	             </td>
	          </tr>
		</table>
	</div>
