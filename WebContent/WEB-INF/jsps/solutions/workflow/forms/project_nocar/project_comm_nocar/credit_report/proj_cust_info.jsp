<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<div class="mini-panel" title="承租企业基本信息" showCollapseButton="true" style="width: 100%;"> 
    <table class="fillTable" cellspacing="0" cellpadding="0">
			<tr class="tr-project-info tr-even">
		        <td class="td-content-title">成立日期
		        </td>
		        <td class="td-content">
		            <input name="companyBaseInfo.a1" class="mini-datepicker" required="true" value="${requestScope['companyBaseInfo.a1'] }" />
		        </td>
		        <td class="td-content-title">注册资本
		        </td>
		        <td class="td-content">
		            <input name="companyBaseInfo.a2" class="mini-textbox" value="${requestScope['companyBaseInfo.a2'] }"/>
		        </td>
		    </tr>
			<tr class="tr-project-info tr-odd">
		        <td class="td-content-title">职工人数
		        </td>
		        <td class="td-content">
		            <input name="companyBaseInfo.a3" class="mini-textbox" required="true"  value="${requestScope['companyBaseInfo.a3'] }"/>
		        </td>
		        <td class="td-content-title" nowrap="nowrap">中高级技术人员人数
		        </td>
		        <td class="td-content">
		            <input name="companyBaseInfo.a4" class="mini-textbox" value="${requestScope['companyBaseInfo.a4'] }"/>
		        </td>
		    </tr>
			<tr class="tr-project-info tr-even">
		        <td class="td-content-title">组织代码
		        </td>
		        <td class="td-content">
		            <input name="companyBaseInfo.a5" class="mini-textbox" required="true"  value="${requestScope['companyBaseInfo.a5'] }"/>
		        </td>
		        <td class="td-content-title">营业执照号
		        </td>
		        <td class="td-content">
		            <input name="companyBaseInfo.a6" class="mini-textbox" value="${requestScope['companyBaseInfo.a6'] }"/>
		        </td>
		    </tr>
			<tr class="tr-project-info tr-odd">
		        <td class="td-content-title">经营范围（主营）
		        </td>
		        <td class="td-content">
		            <input name="companyBaseInfo.a7" class="mini-textbox" required="true"  value="${requestScope['companyBaseInfo.a7'] }"/>
		        </td>
		        <td class="td-content-title">经营范围（兼营）
		        </td>
		        <td class="td-content">
		            <input name="companyBaseInfo.a8" class="mini-textbox" value="${requestScope['companyBaseInfo.a8'] }"/>
		        </td>
		    </tr>
			<tr class="tr-project-info tr-even">
		        <td class="td-content-title">开户银行
		        </td>
		        <td class="td-content">
		            <input name="companyBaseInfo.a9" class="mini-textbox" required="true"  value="${requestScope['companyBaseInfo.a9'] }"/>
		        </td>
		        <td class="td-content-title">银行账号
		        </td>
		        <td class="td-content">
		            <input name="companyBaseInfo.a10" class="mini-textbox" value="${requestScope['companyBaseInfo.a10'] }"/>
		        </td>
		    </tr>
			<tr class="tr-project-info tr-odd">
		        <td class="td-content-title">企业沿革
		        </td>
		        <td colspan="3" class="td-content">
		            <input name="companyBaseInfo.a11" class="mini-textbox" required="true"  value="${requestScope['companyBaseInfo.a11'] }"/>
		        </td>
		    </tr>
		
		</table>
	</div>
 
    
</body>

</html>

