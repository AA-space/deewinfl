<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<div class="mini-panel" title="关联企业及分支机构情况" showCollapseButton="true" style="width: 100%;"> 
    <table class="fillTable" cellspacing="0" cellpadding="0">
			<tr class="tr-even" >
		        <td class="td-content-title">关联企业
		        </td>
		        <td class="td-content">
		            <input name="companyAssociate.a1" class="mini-textbox" required="true" value="${requestScope['companyAssociate.a1'] }"/>
		        </td>
		        <td class="td-content-title">法人/个人
		        </td>
		        <td class="td-content">
		            <input name="companyAssociate.a2" class="mini-textbox" value="${requestScope['companyAssociate.a2'] }"/>
		        </td>
		    </tr>
			<tr class="tr-odd" >
		        <td class="td-content-title">组织机构代码/身份证号
		        </td>
		        <td class="td-content">
		            <input name="companyAssociate.a3" class="mini-textbox" required="true" value="${requestScope['companyAssociate.a3'] }"/>
		        </td>
		        <td class="td-content-title" nowrap="nowrap">关联关系
		        </td>
		        <td class="td-content">
		            <input name="companyAssociate.a4" class="mini-textbox" value="${requestScope['companyAssociate.a4'] }"/>
		        </td>
		    </tr>
			<tr class="tr-even" >
		        <td class="td-content-title">主营业务
		        </td>
		        <td class="td-content">
		            <input name="companyAssociate.a5" class="mini-textbox" required="true" value="${requestScope['companyAssociate.a5'] }"/>
		        </td>
		        <td class="td-content-title">地  址
		        </td>
		        <td class="td-content">
		            <input name="companyAssociate.a6" class="mini-textbox" value="${requestScope['companyAssociate.a6'] }"/>
		        </td>
		    </tr>
			<tr class="tr-odd" >
		        <td class="td-content-title">注册资本
		        </td>
		        <td class="td-content">
		            <input name=companyAssociate.a7 class="mini-textbox" required="true" value="${requestScope['companyAssociate.a7'] }"/>
		        </td>
		        <td class="td-content-title">持股比例
		        </td>
		        <td class="td-content">
		            <input name="companyAssociate.a8" class="mini-textbox" value="${requestScope['companyAssociate.a8'] }"/>
		        </td>
		    </tr>
			<tr class="tr-even" >
		        <td class="td-content-title">法定代表人
		        </td>
		        <td colspan="3" class="td-content">
		            <input name="companyAssociate.a9" class="mini-textbox" required="true" value="${requestScope['companyAssociate.a9'] }"/>
		        </td>
		         
		    </tr>
		
		</table>
	</div>
    
</body>

</html>

