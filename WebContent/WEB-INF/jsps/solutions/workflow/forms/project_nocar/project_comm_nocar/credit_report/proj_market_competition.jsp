<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<div class="mini-panel" title="承租企业现有主要产品市场竞争能力" showCollapseButton="true" style="width: 100%;"> 
    <table class="fillTable" cellspacing="0" cellpadding="0">
			<tr class="tr-even" >
		        <td class="td-content-title">产品名称
		        </td>
		        <td class="td-content">
		            <input name="companyProduct.a1" class="mini-textbox" required="true" value="${requestScope['companyProduct.a1'] }"/>
		        </td>
		        <td class="td-content-title">行业排名
		        </td>
		        <td class="td-content">
		            <input name="companyProduct.a2" class="mini-textbox" value="${requestScope['companyProduct.a2'] }"/>
		        </td>
		    </tr>
			<tr class="tr-odd" >
		        <td class="td-content-title">市场份额
		        </td>
		        <td class="td-content">
		            <input name="companyProduct.a3" class="mini-textbox" required="true" value="${requestScope['companyProduct.a3'] }"/>
		        </td>
		        <td class="td-content-title" nowrap="nowrap">主要市场（辐射）区域
		        </td>
		        <td class="td-content">
		            <input name="companyProduct.a4" class="mini-textbox" value="${requestScope['companyProduct.a4'] }"/>
		        </td>
		    </tr>
			<tr class="tr-even" >
		        <td class="td-content-title">产品价格/产量
		        </td>
		        <td colspan="3" class="td-content">
		            <input name="companyProduct.a5" class="mini-textbox" required="true" value="${requestScope['companyProduct.a5'] }"/>
		        </td>
		    </tr>
			<tr class="tr-odd" >
		        <td class="td-content-title">产品销售前景和销售计划
		        </td>
		        <td colspan="3" class="td-content">
					<input  name="companyProduct.a6" class="mini-textbox" value="${requestScope['companyProduct.a6'] }"/>
		        </td>
		    </tr>
			 <tr class="tr-odd" >
		        <td class="td-content-title">供应商分析
		        </td>
		        <td colspan="3" class="td-content">
					<input  name="companyProduct.a7" class="mini-textbox" value="${requestScope['companyProduct.a7'] }"/>
		        </td>
		    </tr>
		
		</table>
	</div>
    
</body>

</html>

