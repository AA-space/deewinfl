<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<div class="mini-panel" title="租赁物基本情况说明" showCollapseButton="true" style="width: 100%;"> 
    <table class="fillTable" cellspacing="0" cellpadding="0">
			<tr class="tr-even" >
		        <td class="td-content-title">技术寿命及实际可使用年限
		        </td>
		        <td class="td-content">
		            <input name="companyEquipInfo.a1" class="mini-textbox" required="true" value="${requestScope['companyEquipInfo.a1'] }"/>
		        </td>
		        <td class="td-content-title">目前在技术上的领先程度
		        </td>
		        <td class="td-content">
		            <input name="companyEquipInfo.a2" class="mini-textbox" value="${requestScope['companyEquipInfo.a2'] }"/>
		        </td>
		    </tr>
			<tr class="tr-odd" >
		        <td class="td-content-title">通用性和保值/变现能力
		        </td>
		        <td class="td-content">
		            <input name="companyEquipInfo.a3" class="mini-textbox" required="true" value="${requestScope['companyEquipInfo.a3'] }"/>
		        </td>
		        <td class="td-content-title" nowrap="nowrap">租赁物预计到位时间
		        </td>
		        <td class="td-content">
		            <input name="companyEquipInfo.a4" class="mini-textbox" value="${requestScope['companyEquipInfo.a4'] }"/>
		        </td>
		    </tr>
			<tr class="tr-even" >
		        <td class="td-content-title">进口设备完税情况
		        </td>
		        <td class="td-content">
		            <input name="companyEquipInfo.a5" class="mini-textbox" required="true" value="${requestScope['companyEquipInfo.a5'] }"/>
		        </td>
		        <td class="td-content-title">进口免税设备能否办理海关备案
		        </td>
		        <td class="td-content">
		            <input name="companyEquipInfo.a6" class="mini-textbox" value="${requestScope['companyEquipInfo.a6'] }"/>
		        </td>
		    </tr>
			<tr class="tr-odd" >
		        <td class="td-content-title">市场准入、产品合格证办理情况
		        </td>
		        <td class="td-content">
		            <input name="companyEquipInfo.a7" class="mini-textbox" required="true" value="${requestScope['companyEquipInfo.a7'] }"/>
		        </td>
		        <td class="td-content-title">市场上同类品牌的性价比较
		        </td>
		        <td class="td-content">
		            <input name="companyEquipInfo.a8" class="mini-textbox" value="${requestScope['companyEquipInfo.a8'] }"/>
		        </td>
		    </tr>
			<tr class="tr-even" >
		        <td class="td-content-title">其他
		        </td>
		        <td colspan="3" class="td-content">
		            <input name="companyEquipInfo.a9" class="mini-textbox" required="true" value="${requestScope['companyEquipInfo.a9'] }"/>
		        </td>
		         
		    </tr>
		
		</table>
	</div>
    
</body>

</html>

