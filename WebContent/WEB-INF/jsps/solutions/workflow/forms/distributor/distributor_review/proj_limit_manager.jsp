<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<table cellspacing="0" cellpadding="0" id="dealer_credit_info_table">
	<tr class="x-panel-table-div-title">
		<td class="x-panel-table-div-title" colspan=4>
		<div class="toggle-icon-expanded" title="折叠"
			toggleClass="tr-project-per_money"></div>
		经销商授信额度</td>
	</tr>
 <tr class="tr-project-info tr-even" >   
             <td class="td-content-title" style="width:17%">当前授信额度金额：</td><td class="td-content" >
             <input name="dealer_credit_info.creditmoney" dataType="Money" class="td-content-input" require="true" label="当前授信额度金额" type="text"  value="${requestScope['dealer_credit_info.creditmoney'] }">
             <font class="required-content">*</font></td>
             <td  class="td-content-title" style="width:17%"></td><td class="td-content"></td>
              </tr>
         <tr class="tr-project-info tr-odd">
          <td class="td-content-title"> 当前授信额度开始日期：</td><td class="td-content" ><input name="dealer_credit_info.creditstardate" class="Wdate td-content-input td-content-readonly"  
             type="text" dataType="Date" value='${empty requestScope["dealer_credit_info.creditstardate"] ? "" : requestScope["dealer_credit_info.creditstardate"]}'  onClick="WdatePicker(this,{readOnly:true})" readOnly require="true" label="当前授信额度开始日期" /><font class="required-content">*</font></td>
         <td class="td-content-title"> 当前授信额度结束日期：</td><td class="td-content" ><input name="dealer_credit_info.creditenddate" class="Wdate td-content-input td-content-readonly"  
             type="text" dataType="Date" value='${empty requestScope["dealer_credit_info.creditenddate"] ? "" : requestScope["dealer_credit_info.creditenddate"]}'  onClick="WdatePicker(this,{readOnly:true})" readOnly require="true" label="当前授信额度结束日期" /><font class="required-content">*</font></td>
        </tr>
    <tr>
		<td colspan=4 class="td-even-line-separator">&nbsp;</td>
	</tr>
	<tr>
		<td colspan=4 class="td-even-line-separator">&nbsp;</td>
	</tr>
</table>	
